#include <stdio.h>
#include <stdlib.h>
int main()
{
    signed i,p,r,m,j,n,perevod=0;
    printf ("enter a count:\n");
    scanf ("%d",&p);
        printf ("[%d]     ",2);
        for (i=3;i<p;i+=2)
        {
            r=i;
            while (r!=0)
            {
              m=sqrt (r);
              for (j=2;j<=m;j++)
              {
                n=r%j;
                if (n==0)
                {
                    r=0;
                    j=m+1;
                }
              }
              if (r!=0)
              {
              perevod++;
              if (perevod==10)
               {
               printf("[%d]\n",r);
               perevod = 0;
               }
              else
              printf ("[%d]     ",r);
              }
              r=0;

            }
        }
    return 0;
}
